ction () {\n                    alert('认证中心异常');\n                });\n            }\n        },\n        created() {\n            this.LoadData()\n        },\n        mounted() {\n\n        },\n        updated() {\n        }\n    }\n</script>\n\n<style lang=\"sass\">\n\n</style>\n"],"sourceRoot":"src/views/Login"}]}