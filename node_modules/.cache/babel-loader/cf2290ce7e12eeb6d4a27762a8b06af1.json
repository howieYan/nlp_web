,KAAA,QAAA,CAAA,GAAA,EAAA;AACA,aAAA,IAAA,GAAA,EAAA;AACA,aAAA,QAAA,GAAA,MAAA,CAAA,GAAA;AACA,aAAA,QAAA,CAAA,MAAA;AACA,OAJA,MAIA;AACA,aAAA,QAAA,CAAA,KAAA,CAAA,QAAA;AACA;AAEA,KAVA;AAWA,IAAA,QAXA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAaA,qBAAA,SAAA,GAAA,IAAA;AAbA;AAAA,uBAcA,GAAA,CAAA,IAAA,CAAA;AAAA,kBAAA,GAAA,EAAA,KAAA,QAAA,CAAA;AAAA,iBAAA,CAdA;;AAAA;AAcA,gBAAA,MAdA;AAeA,gBAAA,OAfA,GAeA,IAAA,CAAA,SAAA,CAAA,MAAA,CAAA,IAAA,EAAA,IAAA,EAAA,CAAA,CAfA;AAgBA,gBAAA,OAAA,CAAA,OAAA,CAAA,MAAA,EAAA,EAAA;AACA,qBAAA,IAAA,GAAA,OAAA;AACA,qBAAA,SAAA,GAAA,KAAA;AAlBA;AAAA;;AAAA;AAAA;AAAA;AAoBA,gBAAA,OAAA,CAAA,GAAA;;AApBA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;;AAAA;AAAA;AAuBA,IAAA,YAvBA,0BAuBA;AACA,WAAA,KAAA,CAAA,KAAA,QAAA,EAAA,CAAA,EAAA,QAAA,CAAA,KAAA,QAAA;AACA;AAzBA,GA7BA;AAwDA,EAAA,OAxDA,qBAwDA,CACA,CAzDA;AA2DA,EAAA,OA3DA,qBA2DA,CACA,CA5DA;AA6DA,EAAA,OA7DA,qBA6DA,CACA;AA9DA,CAAA","sourcesContent":["<template>\n    <div class=\"Layout\">\n        <div class=\"sorcoll\">\n            <div class=\"wrapper\">\n                <div class=\"nav\">\n                    <div class=\"paddTop\">\n                        <div class=\"row\">\n                            <div class=\"col\">\n                                <Form :model=\"formData\" :label-width=\"30\">\n                                    <FormItem label=\"URL\">\n                                        <Input v-model=\"formData.url\" :autosize=\"{minRows: 4,maxRows: 4}\"  placeholder=\"\"></Input>\n                                    </FormItem>\n                                </Form>\n                            </div>\n                            <div style=\"padding-left: 10px\">\n                                <Button type=\"primary\" @click=\"AutomaticAccess\">自动清洗</Button>\n                            </div>\n                        </div>\n\n                    </div>\n                    <div style=\"position: relative;min-height: 300px\">\n                        <div v-if=\"!isLoading\">\n                            <div style=\"border: 1px solid #dcdee2;min-height: 300px;padding: 10px\">\n                                <pre style=\"word-wrap: break-word; white-space: pre-wrap;margin: 0\">{{data}}</pre>\n                            </div>\n                        </div>\n<!--                        <Input v-model=\"data\" type=\"textarea\" :autosize=\"{minRows: 15,maxRows: 15}\" placeholder=\"\" v-if=\"!isLoading\"/>-->\n                        <Spin fix v-else>\n                            <Icon type=\"ios-loading\" size=18 class=\"demo-spin-icon-load\"></Icon>\n                            <div>努力加载中...</div>\n                        </Spin>\n                    </div>\n\n                </div>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\n    import api from \"./api/api\";\n    import Headers from \"../../components/Headers\";\n    export default {\n        name: \"cleaning\",\n        components: {Headers},\n        data() {\n            return {\n                industry: '0',\n                formData: {\n                    url: '',\n                },\n                isLoading: false,\n                index: 0,\n                tabSName: '/toutiao',\n                navList: [\n                    {name: '今日头条解析', api: '/toutiao'},\n                    {name: '天天快报解析', api: '/ttkb'},\n                    {name: '网络通用解析', api: '/web'},\n                    {name: '微博解析', api: '/weibo'},\n                    {name: '微信解析', api: '/weixin'},\n                    {name: '一点资讯解析', api: '/ydzx'},\n                    {name: '知乎解析', api: '/zhihu'},\n                ],\n                data: ''\n            };\n        },\n        computed: {\n            routers () {\n                return this.$route.path\n            }\n        },\n        methods: {\n            AutomaticAccess (record) {\n                if (this.formData.url) {\n                    this.data = ''\n                    this.tabSName = record.api\n                    this.LoadData(record)\n                } else {\n                    this.$Message.error('请输入url');\n                }\n\n            },\n            async LoadData() {\n                try {\n                    this.isLoading = true\n                    let result = await api.List({url: this.formData.url})\n                    let jsonStr = JSON.stringify(result.data,null,4);\n                    jsonStr.replace(/\\s+/g,\"\");\n                    this.data = jsonStr\n                    this.isLoading = false\n                } catch (e) {\n                    console.log(e)\n                }\n            },\n            handleSubmit () {\n                this.$refs[this.tabSName][0].LoadData(this.formData)\n            }\n        },\n        created() {\n        },\n\n        mounted() {\n        },\n        updated() {\n        }\n    }\n</script>\n\n<style lang=\"scss\">\n    @import \"../../css/iconFontSize/iconfont.css\";\n    .Layout{\n        .sorcoll{\n            height: calc(100vh - 80px);\n            overflow-y: auto;\n        }\n        .wrapper{\n            .nav{\n                .el-tabs__item{\n                    padding: 0 15px;\n                }\n            }\n            .paddTop{\n                padding-top: 20px;\n            }\n        }\n        .footer{\n            height: 50px;\n        }\n        .ivu-form-item{\n            margin-bottom: 15px;\n        }\n    }\n    .ivu-btn-text:hover{\n        color: #19be6b;\n    }\n    .row{\n        display: flex;\n    }\n    .col{\n        flex: 1;\n    }\n</style>\n"],"sourceRoot":"src/views/cleaning"}]}